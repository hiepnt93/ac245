<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="whill_facility_facility" resource="default" engine="innodb" comment="whill_facility_facility Table">
		<column xsi:type="int" name="facility_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="facility_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="varchar" comment="name" length="255"/>
		<column name="lat" nullable="true" xsi:type="varchar" comment="lat" length="255"/>
		<column name="long" nullable="true" xsi:type="varchar" comment="long" length="255"/>
	</table>
</schema>
